{
    "version": 2,
    "builds": [
      {
        "src": "dist/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/docs(.*)", 
        "dest": "dist/index.js"
      },
      {
        "src": "/add-three-products-to-cart", 
        "dest": "dist/index.js"
      },
      {
        "src": "/(.*)", 
        "dest": "dist/index.js"
      }
    ]
  }
  